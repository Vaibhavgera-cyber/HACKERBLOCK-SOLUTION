#include<iostream>
using namespace std;
int main() {
    bool flag=false;
	int n,m;
	cin>>n>>m;
	int arr[n][m];
	
	for(int i=0;i<n;i++){
		for(int j=0;j<m;j++){
			cin>>arr[i][j];
		}
	}
	int x;
	cin>>x;

	for(int i=0;i<n;i++){
		for(int j=0;j<m;j++){
			if(x==arr[i][j]){
				flag=true;
			}
			
		}
	}
	
	if(flag){
	    cout<<"1";
	}
	else{
	    cout<<"0";
	}



	return 0;
}
