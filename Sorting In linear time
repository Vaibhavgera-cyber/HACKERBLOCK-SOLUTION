#include<iostream>
using namespace std;
int main() {
	int two=0,one=0,zero=0;
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++){
		cin>>arr[i];
		if(arr[i]==1){
			one++;
		}
		else if(arr[i]==2){
			two++;
		}
		else{
			zero++;
		}
	}

	for(int i=0;i<zero;i++){
		cout<<0<<" ";
	}
	for(int i=0;i<one;i++){
		cout<<1<<" ";
	}
	for(int i=0;i<two;i++){
		cout<<2<<" ";
	}


	return 0;
}
