#include <iostream>
#include<bits/stdc++.h>

using namespace std;

int main()
{
    // int n;
    // cin>>n;
    // char arr[n+1];
    // cin>>arr;
   
   
    string str;
    getline(cin,str);
    int n=str.size();
    bool check=1;
    for(int i=0;i<n;i++)
    {
        if(str[i]!=str[n-1-i]){
            check=0;
            break;
        }
    }
    
    if(check)
    
        cout<<"true";
        
    
    else
    {
        cout<<"false";
    }
    return 0;
}
